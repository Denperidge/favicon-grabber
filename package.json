{
  "name": "favicon-grabber",
  "description": "A Node.js no-dependency favicon grabber. It doesn't rely on external endpoints unless nothing else works; instead opting to use the HTML, regex and fetch wherever possible",
  "version": "0.4.0",
  "type": "module",
  "main": "favicon-grabber.js",
  "author": "Denperidge",
  "license": "MIT",
  "scripts": {
    "test": "ava --timeout=2m"
  },
  "directories": {
    "test": "tests"
  },
  "homepage": "https://github.com/Denperidge/favicon-grabber#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Denperidge/favicon-grabber.git"
  },
  "bugs": {
    "url": "https://github.com/Denperidge/favicon-grabber/issues"
  },
  "keywords": [
    "favicons"
  ],
  "devDependencies": {
    "@types/node": "^22.14.0",
    "ava": "^6.2.0",
    "file-type-mime": "^0.4.6"
  },
  "ava": {
    "environmentVariables": {
      "DEBUG_FAVICON_GRABBER": "1"
    }
  }
}
